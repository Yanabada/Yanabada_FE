{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "endOfLine": "auto",
  "importOrder": [
    "^@apis/(.)$",
    "^@assets/(.)$",
    "^@components/(.)$",
    "^@constants/(.)$",
    "^@hooks/(.)$",
    "^@pages/(.)$",
    "^@stores/(.)$",
    "^@styles/(.*)$",
    "^@/types/(.)$",
    "^@tests/(.)$",
    "^themes/(.)$",
    "^@utils/(.)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
